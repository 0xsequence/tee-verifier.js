{
  "name": "@0xsequence/tee-verifier",
  "version": "0.1.1",
  "license": "Apache-2.0",
  "type": "module",
  "description": "A library for verifying TEE enclave attestation",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "format": "prettier --list-different --write \"src/**/*.{ts,tsx,md}\""
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "tee",
    "attestation",
    "nitro",
    "enclave"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/0xsequence/tee-verifier.js.git"
  },
  "bugs": {
    "url": "https://github.com/0xsequence/tee-verifier.js/issues"
  },
  "homepage": "https://github.com/0xsequence/tee-verifier.js#readme",
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "lefthook": "^1.11.11",
    "prettier": "^3.5.3",
    "typescript": "^5.7.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "cbor2": "^1.12.0",
    "pkijs": "^3.2.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "lefthook"
    ]
  }
}
